{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "WorkspaceLauncher",
  "version": "0.3.4",
  "identifier": "io.w4y.workspacelauncher",
  "build": {
    "beforeDevCommand": "bun run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "bun run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "WorkspaceLauncher",
        "width": 1200,
        "height": 800,
        "minWidth": 1000,
        "minHeight": 750
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "createUpdaterArtifacts": true,
    "icon": [
      "icons/32x32.png",
      "icons/64x64.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "shell": {
      "open": true
    },
    "updater": {
      "active": true,
      "endpoints": [
        "https://github.com/ur-wesley/WorkspaceLauncher/releases/latest/download/latest.json"
      ],
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDZDNUUxNEQ0NzdDQ0M5QUMKUldTc3ljeDMxQlJlYk9rNkl2TW9ac3hrSHRqcXZ1YVNlMjRpbkJXbUdINXU5K29heGl1K2ZvOWgK"
    }
  }
}